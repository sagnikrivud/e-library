{
  "name": "jeroennoten/laravel-adminlte",
  "description": "Easy AdminLTE integration with Laravel",
  "keywords": ["laravel", "adminlte", "admin", "administrator"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jeroen Noten",
